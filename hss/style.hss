html, body {
	margin: 0;
	padding: 0;
}

#haxe_log {
	position: fixed; 
	left: 0; 
	right: 0; 
	bottom: 0; 
	height: 160px; 
	white-space: pre; 
	font-size: 14px; 
	font-family: monospace; 
	background-color: transparent; 
	color: #333; 
	border: 1px solid #ddd; 
	overflow: auto;
}